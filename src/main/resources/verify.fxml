<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>
<VBox
    maxHeight="-Infinity"
    maxWidth="-Infinity"
    minHeight="-Infinity"
    minWidth="-Infinity"
    prefHeight="800.0"
    spacing="10.0"
    xmlns="http://javafx.com/javafx/8.0.281"
    xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="ru.fusionsoft.database.application.gui.VerifyControls"
>
    <children>
        <ScrollPane fx:id="scroll">
            <content>
                <TextFlow fx:id="flow" snapToPixel="false"/>
            </content>
        </ScrollPane>
        <HBox>
            <Label fx:id="status" text="Label"/>
            <ButtonBar HBox.hgrow="ALWAYS" maxWidth="Infinity" prefHeight="40.0">
                <buttons>

                    <Button fx:id="save" mnemonicParsing="false" onMouseClicked="#save" text="Save"/>
                    <Button fx:id="exit" mnemonicParsing="false" onMouseClicked="#exit" text="Exit"/>
                </buttons>
            </ButtonBar>
        </HBox>

    </children>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
